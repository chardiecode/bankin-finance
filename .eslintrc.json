{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],

  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "noSortAlphabetically": false,
        "reservedFirst": true
      }
    ],
    "prettier/prettier": "error"
  }
}
